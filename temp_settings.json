{
  "[cairo]": {
    "editor.defaultFormatter": "StarkWare.cairo1"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[mdx]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "code-runner.runInTerminal": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.wordWrap": "on",
  "editor.accessibilitySupport": "off",
  "editor.inlineSuggest.enabled": true,
  "editor.largeFileOptimizations": true,
  "git.confirmSync": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.env.osx": {
    "CW_NEW_SESSION": "1",
    "FIG_NEW_SESSION": "1",
    "Q_NEW_SESSION": "1"
  },

  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.tree.renderIndentGuides": "none",
  "git.autofetch": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "sqlite3-editor.ui.tableSelector.displayColumns": true,
  "sqlite3-editor.ui.tableSelector.displayShadowTables": true,
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.hidesExplorerArrows": true,
  "workbench.panel.defaultLocation": "bottom",
  "workbench.sideBar.location": "right",
  "workbench.statusBar.visible": false,
  "window.zoomLevel": 1.05,
  "workbench.colorCustomizations": {
    "editor.background": "#070212",
    "sideBar.background": "#070212",
    "terminal.background": "#070212"
  },
  "editor.minimap.enabled": false,
  "editor.guides.indentation": false,
  "editor.overviewRulerBorder": false,
  "explorer.compactFolders": false,
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.fontSize": 11,
  "editor.fontLigatures": true,
  "editor.lineHeight": 1.8,
  "terminal.integrated.fontFamily": "'FiraCode Nerd Font Mono'",
  "window.titleBarStyle": "custom",

  "workbench.activityBar.location": "hidden",

  "editor.unicodeHighlight.invisibleCharacters": false,

  "terminal.integrated.profiles.osx": {
    "bash": {
      "path": "bash",
      "args": ["-l"],
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh",
      "args": ["-l"]
    },
    "fish": {
      "path": "fish",
      "args": ["-l"]
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    },
    "pwsh": {
      "path": "pwsh",
      "icon": "terminal-powershell"
    }
  },
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "Cargo.toml": "Cargo.lock",
    "Scarb.toml": "Scarb.lock",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "files.autoSave": "afterDelay",
  "workbench.colorTheme": "Aura Dracula Spirit",
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "breadcrumbs.enabled": false
}
